using System;

namespace task1
{
    class Program
    {
        static void Main(string[] args)
        {
            var sum = 0;
            for (var i=0;i<1000;i++)
            {
                if (i % 5 == 0 || i % 3 == 0)
                    sum = sum + i;
            }
            Console.WriteLine(sum);
        }
    }
}
